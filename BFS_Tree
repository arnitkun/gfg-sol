void levelOrder(Node* node)
{
  //Your code here
  if(!root) return;
  queue<Node*> q;
  q.push(node);
  Node* temp;
  
  while(!q.empty())
  { 
    temp = q.front();
    cout<<temp->data<<" ";
    q.pop();
    if(temp->left) q.push(temp->left);
    if(temp->right) q.push(temp->right);
  }
    return;
}
